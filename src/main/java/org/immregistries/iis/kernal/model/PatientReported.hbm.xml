<?xml version="1.0"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">
<hibernate-mapping package="org.immregistries.iis.kernal.model">
  <class name="PatientReported" table="patient_reported">
    <id name="reportedPatientId" column="reported_patient_id">
      <generator class="native"/>
    </id>
    <many-to-one name="reportedOrg" column="reported_org_id" class="OrgMaster"/>
    <property name="reportedMrn" column="reported_mrn" type="string"/>
    <many-to-one name="Patient" column="patient_id" class="PatientMaster"/>
    <property name="patientData" column="patient_data" type="string"/>
    <property name="reportedDate" column="reported_date" type="date"/>
    <property name="updatedDate" column="updated_date" type="date"/>
  </class>
</hibernate-mapping>